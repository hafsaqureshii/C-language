#include<stdio.h>
void addition(int a, int b);
void subtraction(int a , int b);
void multiplication(int a, int b);
main()
{
	addition(3,6);
	subtraction(3,6);
	multiplication(3,6);
}
void addition (int a, int b )
{

	printf("Addition is %d + %d =%d\n",a,b,a+b);
}
void subtraction (int a , int b)
 
 {
	   printf("Subtraction is %d -%d =%d\n ",a,b,a-b);

}
void multiplication (int a , int b)
	{
	   printf("Multiplication is %d * %d =%d \n",a,b,a*b);
}
